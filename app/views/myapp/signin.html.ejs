<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="/stylesheets/screen.css" />
    <link rel="stylesheet" href="/stylesheets/bootstrap.min.css">

    <script src="/lib/angular.js"></script>

    <script src="/js/app.js"></script>
    <script src="/js/controllers/signInController.js"></script>

  </head>
  <body ng-app="sample">
    <h3 class="text-center">Sign In Sample From the MongoDb</h3>

    <form ng-controller="signInController">
      <div class="form-group">
        <label for="exampleInputEmail1">Username</label>
        <input type="text" class="form-control" id="" placeholder="Enter username" ng-model="userData.username">
      </div>

      <div class="form-group">
        <label for="exampleInputPassword1">Password</label>
        <input type="password" class="form-control" id="exampleInputPassword1" placeholder="password" ng-model="userData.password">
      </div>

      <button type="submit" class="btn btn-default" ng-click="submit()">Submit</button>
    </form>
  </body>
</html>